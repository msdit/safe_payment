{
  "author": "Masoud Aghaei",
  "name": "Safe Payment | پرداخت امن",
  "version": "2.0.1",
  "description": "پرداخت امن | امنیت پرداخت در درگاه‌های پرداخت اینترنتی",
  "permissions": ["tabs"],
  "background": {
    "scripts": ["assets/js/background.js"]
  },
  "content_scripts": [ {
    "exclude_globs": [ "*://www.shaparak.ir/*", "*://shaparak.ir/*" ],
    "js": [ "assets/js/content-script.js" ],
    "matches": [ "https://*.shaparak.ir/*" ]
  } ],
  "browser_action": {
    "default_icon": {
      "16": "assets/logo/logo16.png",
      "32": "assets/logo/logo32.png",
      "48": "assets/logo/logo48.png",
      "128": "assets/logo/logo128.png"
    },
    "default_popup": "popup.html",
    "default_title": "Safe Payment | پرداخت امن"
  },
  "icons": {
    "16": "assets/logo/logo16.png",
    "32": "assets/logo/logo32.png",
    "48": "assets/logo/logo48.png",
    "128": "assets/logo/logo128.png"
  },
  "manifest_version": 2,
  "key": ""
}
